<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="CollapsingLayout">
		<attr name="backgroundLayout" format="reference" />
		<attr name="infoLayout" format="reference" />
		<attr name="headHeight" format="dimension" />
		<attr name="head_backgroundParallaxMultiplier" format="float" />
		<attr name="head_infoParallaxMultiplier" format="float" />
		<attr name="head_isGradient" format="boolean" />
		<attr name="head_gradientBackground" format="reference|color" />
	</declare-styleable>
	<declare-styleable name="LikeView">
		<attr name="font" format="reference"/>
		<attr name="order" >
			<enum name="reverse" value="-1" />
			<enum name="positive" value="1" />
		</attr>
		<attr name="likeIcon" format="reference" />
	</declare-styleable>
    <declare-styleable name="NoScrollViewPager">
		<attr name="pagingEnable" format="boolean"/>
	</declare-styleable>
    <declare-styleable name="TitleTextView">
		<attr name="titleLetterSpacing" format="float"/>
	</declare-styleable>
</resources>